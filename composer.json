{
    "name": "quanta/container-configuration",
    "type": "library",
    "description": "Library for creating quanta Psr-11 container factory maps from service providers",
    "keywords": [
        "psr-11",
        "container",
        "dependency-injection",
        "service-provider"
    ],
    "license": "MIT",
    "homepage": "https://github.com/quantaphp/container-configuration",
    "support": {
        "issues": "https://github.com/quantaphp/container-configuration/issues",
        "source": "https://github.com/quantaphp/container-configuration"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.0",
        "container-interop/service-provider": "^0.4.0",
        "quanta/class-names": "^0.2.0",
        "quanta/exceptions": "^0.4.0"
    },
    "require-dev": {
        "eloquent/phony-kahlan": "^2.0",
        "kahlan/kahlan": "^4.0",
        "phpstan/phpstan": "^0.10.8",
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Quanta\\Container\\": "src"
        }
    }
}
